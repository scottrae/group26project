+import java.io.IOException;
+import javax.servlet.ServletException;
+import javax.servlet.annotation.WebServlet;
+import javax.servlet.http.HttpServlet;
+import javax.servlet.http.HttpServletRequest;
+import javax.servlet.http.HttpServletResponse;
+import javax.swing.JFrame;
+import javax.swing.JScrollPane;
+import java.awt.Graphics2D;
+import java.awt.image.BufferedImage;
+import java.awt.*;
+import java.io.FileOutputStream;
+import net.jmge.gif.Gif89Encoder;
+@WebServlet("/GroupStuff")
+public class groupproject extends HttpServlet {
+	private static final long serialVersionUID = 1L;
+       
+    
+    public groupproject() {
+        super();
+    }
 
+
+	protected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
+		int r = 20;
+		BufferedImage image = new BufferedImage(150,200, BufferedImage.TYPE_INT_ARGB);
+		Graphics2D g = image.createGraphics();
+		  Polygon star = new Polygon(
+		   new int[]{0,22,95,36,59,0,-59,-36,-95,-22},
+		   new int[]{-100,-31,-31,12,81,38,81,12,-31,-31},10);
+		  g.setColor(Color.red);
+		  g.fillRect(0,0,300,200);
+		  g.setColor(Color.yellow);
+		  g.translate(75,50);
+		  g.scale(r/100.0,r/100.0);
+		  JFrame frame = new JFrame("Google");
+	        frame.add(new JScrollPane());
+	        frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
+	        frame.setVisible(true);
+	        frame.pack();
+		  g.setRenderingHint(
+		    RenderingHints.KEY_ANTIALIASING,
+		    RenderingHints.VALUE_ANTIALIAS_ON);
+		  g.fillPolygon(star);
+		}
+     //THIS IS FOR THE drawFrame below. 
+	static void drawFrame(Graphics2D g, int frame){
+		  g.setColor(Color.green);
+		  g.fillRect(0,0,150,100);
+		  g.setColor(Color.yellow);
+		  g.fillRect(50,8*frame-20,50,20);
+		}
+	protected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
+	
+		try {	
+		        Gif89Encoder genc = new Gif89Encoder(); 
+		        for (int i=0;i<20;i++){
+		          BufferedImage image = new BufferedImage(150,100, BufferedImage.TYPE_INT_ARGB);
+		          Graphics2D g = image.createGraphics();
+		          drawFrame(g,i);
+		          genc.addFrame(image);
+		          g.dispose();
+		        }
+		        genc.setUniformDelay(10);
+		        genc.setLoopCount(0);
+		        //TAKE OUT WRITE TO RESPONSE.SOMETHING
+		        FileOutputStream out = new FileOutputStream("anim.gif");
+		        genc.encode(out);
+		    }catch(Exception e){
+		     System.err.println(e);
+		    }
+		  }
+	}
